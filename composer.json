{
    "name": "pggns/cakephp-cleaner-bake",
    "description": "CakePHP plugin that adds some cleaner Bake Templates",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "~4.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "CleanerBake\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CleanerBake\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
